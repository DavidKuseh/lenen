<div>
    <% if(books != null){ %>
        <% books.forEach(book => { %>
            <a href="/api/books/<%= book.id %>">
                <h3><%= book.title %></h3>
                <h4><%= book.author %></h4>
                <p><%= book.year_published %></p>
                <p><%= book.category %></p>
            </a>
            <% }) %>
        <% } else { %>
            <p>No books available</p>
    <% } %>
</div>
